{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": false,
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@images/*": [
        "/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}